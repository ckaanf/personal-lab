input LectureInput {
    title: String!
    description: String!
}

type Lecture {
    id: ID!
    title: String!
    description: String!
    enrolled: Boolean!
}

type Query {
    lectures: [Lecture!]!
}

type Mutation {
    addLecture(input: LectureInput!): Lecture!
    enrollLecture(id: ID!): Lecture!
}
